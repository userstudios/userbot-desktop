{
    "name": "userbot-desktop",
    "productName": "UBDesktop",
    "description": "A desktop app which is provided by UserStudios",
    "version": "1.0.4",
    "license": "SEE LICENCE IN LICENCE.md",
    "main": "src/app.js",
    "scripts": {
        "start": "electron .",
        "dist": "electron-builder",
        "publish": "electron-builder -p always"
    },
    "keywords": [],
    "author": {
        "name": "UserStudios"
    },
    "config": {},
    "dependencies": {
        "electron-log": "^4.3.5",
        "electron-squirrel-startup": "^1.0.0",
        "electron-updater": "^4.3.8",
        "electron-wix-msi": "^3.2.0",
        "exe-icon-extractor": "^1.0.8",
        "extract-zip": "^2.0.1",
        "fs": "0.0.1-security",
        "glob": "^7.1.6",
        "jarfile": "^2.0.0",
        "node-fetch": "^2.6.1",
        "node-gyp": "^8.0.0",
        "open": "^7.4.2",
        "request": "^2.88.2",
        "trash": "^7.1.1"
    },
    "devDependencies": {
        "electron": "11.1.1",
        "electron-builder": "^22.10.5"
    },
    "build": {
        "publish": [{
            "provider": "github",
            "owner": "0xtcb",
            "repo": "userbot-desktop"
        }],
        "appId": "com.github.0xtcb.userbot-desktop",
        "copyright": "UserStudios 2021",
        "win": {
            "publish": [
                "github"
            ],
            "target": [{
                "target": "nsis",
                "arch": [
                    "x64"
                ]
            }],
            "icon": "./src/data/app.ico",
            "publisherName": [
                "UserStudios"
            ]
        },
        "nsis": {
            "installerIcon": "./src/data/app.ico",
            "uninstallerIcon": "./src/data/app.ico",
            "uninstallDisplayName": "Uninstall UBDesktop",
            "license": "LICENCE.md",
            "oneClick": false,
            "displayLanguageSelector": true,
            "allowToChangeInstallationDirectory": true
        }
    },
    "repository": "github:0xtcb/userbot-desktop"
}